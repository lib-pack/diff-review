{
	"name": "diff-review",
	"version": "0.1.14",
	"description": "a code diff review tools!!!",
	"repository": {
		"type": "git",
		"url": "https://github.com/lib-pack/diff-review"
	},
	"license": "MIT",
	"author": {
		"name": "WumaCoder",
		"email": "1829913225@qq.com"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/diff-review.js",
			"require": "./lib/diff-review.cjs"
		}
	},
	"main": "./lib/diff-review.cjs",
	"module": "./lib/diff-review.js",
	"types": "./lib/src/index.d.ts",
	"bin": {
		"diff-review": "./bin/index.js"
	},
	"files": [
		"bin/",
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "vite build",
		"diff-review": "node bin/index.js",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package": "npmPkgJsonLint .",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky install",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@liuli-util/vite-plugin-node": "^0.6.0",
		"commander": "^11.0.0",
		"cosmiconfig": "^8.2.0",
		"eslint": "^8.46.0",
		"espree": "^10.0.1",
		"gitlog": "^4.0.8",
		"glob": "^10.3.3",
		"lodash.merge": "^4.6.2",
		"matcher": "^5.0.0",
		"openai": "^3.3.0",
		"simple-git": "^3.19.1",
		"throat": "^6.0.2",
		"vite": "^4.4.8"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.1.0",
		"@types/eslint": "^8.37.0",
		"@types/lodash.merge": "^4.6.7",
		"@types/node": "^20.4.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-istanbul": "^0.34.0",
		"console-fail-test": "^0.2.3",
		"cspell": "^6.31.1",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-deprecation": "^1.4.1",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsdoc": "^46.0.0",
		"eslint-plugin-jsonc": "^2.8.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-n": "^16.0.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^1.4.0",
		"eslint-plugin-regexp": "^1.15.0",
		"eslint-plugin-vitest": "^0.2.2",
		"eslint-plugin-yml": "^1.7.0",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.3.0",
		"knip": "2.17.3",
		"lint-staged": "^13.2.2",
		"markdownlint": "^0.29.0",
		"markdownlint-cli": "^0.41.0",
		"npm-package-json-lint": "^7.0.0",
		"npm-package-json-lint-config-default": "^6.0.0",
		"prettier": "^3.0.1",
		"prettier-plugin-curly": "^0.1.2",
		"prettier-plugin-packagejson": "^2.4.3",
		"release-it": "^16.0.0",
		"sentences-per-line": "^0.2.1",
		"should-semantic-release": "^0.1.1",
		"typescript": "^5.0.4",
		"vite-plugin-dts": "^3.5.1",
		"vite-plugin-esmodule": "^1.5.0",
		"vitest": "^0.33.0",
		"yaml-eslint-parser": "^1.2.2"
	},
	"packageManager": "pnpm@8.6.11",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
